{
  "manifest_version": 3,
  "name": "GameStopBurnUpdater/",
  "version": "0.1",
  "host_permissions": ["https://nft.gamestop.com/token*"],
  "web_accessible_resources": [{
    "resources" : ["helpers.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": [
        "https://nft.gamestop.com/token*"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "icons": { "48": "kostika.png" }
}
